<form method="post" action="{{ path('vente_delete', {'idvente': vente.idvente}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ vente.idvente) }}">
    <button class="btn">Supprimer</button>
</form>
